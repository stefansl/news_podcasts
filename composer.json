{

  "name": "stefansl/news_podcasts",
  "description": "Add audio files to your news and let Contao generate podcast compatible RSS",
  "keywords": ["contao", "extension", "podcast", "itunes", "RSS", "Podcast-Feed"],
  "type": "contao-module",
  "homepage": "https://github.com/stefansl/news_podcasts",
  "license": "LGPL-3.0+",
  "authors": [
    {
      "name": "Stefan Schulz-Lauterbach",
      "homepage": "http://clickpress.de",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/stefansl/news_podcasts/issues",
    "source": "https://github.com/stefansl/news_podcasts"
  },
  "require": {
    "php": ">=5.3.2",
    "contao/core-bundle":"~3.2 || ~4.1",
    "contao-community-alliance/composer-plugin":"~2.4 || ~3.0"
  },
  "autoload": {
    "classmap": [
      "classes/"
    ]
  },
  "replace": {
    "contao-legacy/news_podcasts": "self.version",
    "clickpress/news_podcasts": "self.version"
  },
  "extra": {
    "contao": {
      "sources": {
        "": "system/modules/news_podcasts"
      },
      "transifex": {
        "project": "news_podcasts",
        "prefix": "",
        "languages_cto": "languages",
        "languages_tx": ".tx"
      }
    }
  }
}
